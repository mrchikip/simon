<script src="/socket.io/socket.io.js"></script>

<div class="container p-4" id="mcards">
    <div class="row">
        {{#each list}}
        <div class="col-md-3 mx-auto g-2">
            <div class="card-deck mt-3">
                {{#if diferencia }}
                {{#if valor}}
                {{> workingCard }}
                {{else}}
                {{> stopCard }}
                {{/if}}
                {{else}}
                {{> offlineCard }}
                {{/if}}
            </div>
        </div>
        {{/each}}
    </div>
</div>